<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0-windows</TargetFramework>
		<OutputType>WinExe</OutputType>
		<RootNamespace>RenameFiles</RootNamespace>
		<AssemblyName>RenameFiles</AssemblyName>
		<Version>1.1.0</Version>
		<Authors>Pierre Violent</Authors>
		<Title>Quick rename file application</Title>
		<Description>Dot Net version of VB6 RenameFiles</Description>
		<Copyright>Copyright ©2006-2022 Pierre Violent</Copyright>
		<ApplicationIcon>RenameFiles.ico</ApplicationIcon>
		<UseWindowsForms>true</UseWindowsForms>
		<MyType>WindowsForms</MyType>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>
	<ItemGroup>
		<Import Include="System.Data" />
		<Import Include="System.Drawing" />
		<Import Include="System.Windows.Forms" />
	</ItemGroup>
	<ItemGroup>
		<Compile Update="My Project\Application.Designer.vb">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Application.myapp</DependentUpon>
		</Compile>
	</ItemGroup>
	<ItemGroup>
		<None Update="My Project\Application.myapp">
			<Generator>MyApplicationCodeGenerator</Generator>
			<LastGenOutput>Application.Designer.vb</LastGenOutput>
		</None>
	</ItemGroup>
	<ItemGroup>
		<Folder Include="My Project" />
	</ItemGroup>
</Project>