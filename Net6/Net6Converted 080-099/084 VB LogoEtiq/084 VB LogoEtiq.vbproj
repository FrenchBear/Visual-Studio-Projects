<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0-windows</TargetFramework>
        <OutputType>WinExe</OutputType>
        <RootNamespace>VB084</RootNamespace>
        <AssemblyName>VB084</AssemblyName>
        <Version>1.0.0</Version>
        <Authors>Pierre Violent</Authors>
        <Copyright>Copyright ©2003-2021 Pierre Violent</Copyright>
        <UseWindowsForms>true</UseWindowsForms>
        <StartupObject>VB084.frmAnalyse</StartupObject>
		<MyType>WindowsForms</MyType>
	</PropertyGroup>

	<ItemGroup>
		<Import Include="System.Data" />
		<Import Include="System.Drawing" />
		<Import Include="System.Windows.Forms" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="My Project\Application.Designer.vb">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Application.myapp</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="My Project\" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="LogoSGS.bmp">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>

</Project>