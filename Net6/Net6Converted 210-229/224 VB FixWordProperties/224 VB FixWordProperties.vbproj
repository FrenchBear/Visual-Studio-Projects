<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0-windows</TargetFramework>
        <OutputType>WinExe</OutputType>
        <RootNamespace>FixWordProperties</RootNamespace>
        <AssemblyName>FixWordProperties</AssemblyName>
        <Version>1.0.0</Version>
        <Authors>Pierre Violent</Authors>
        <Title>FixWordProperties</Title>
        <Company>KNG Consulting, Inc.</Company>
        <Copyright>Copyright ©2021 Pierre Violent</Copyright>
        <UseWindowsForms>true</UseWindowsForms>
        <MyType>WindowsForms</MyType>
    </PropertyGroup>
    <ItemGroup>
      <COMReference Include="Microsoft.Office.Interop.Word">
        <WrapperTool>tlbimp</WrapperTool>
        <VersionMinor>7</VersionMinor>
        <VersionMajor>8</VersionMajor>
        <Guid>00020905-0000-0000-c000-000000000046</Guid>
        <Lcid>0</Lcid>
        <Isolated>false</Isolated>
        <EmbedInteropTypes>true</EmbedInteropTypes>
      </COMReference>
    </ItemGroup>
    <ItemGroup>
        <Import Include="System.Data" />
        <Import Include="System.Drawing" />
        <Import Include="System.Windows.Forms" />
    </ItemGroup>
    <ItemGroup>
        <Compile Update="My Project\Application.Designer.vb">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>Application.myapp</DependentUpon>
        </Compile>
    </ItemGroup>
    <ItemGroup>
        <None Update="My Project\Application.myapp">
            <Generator>MyApplicationCodeGenerator</Generator>
            <LastGenOutput>Application.Designer.vb</LastGenOutput>
        </None>
    </ItemGroup>
    <ItemGroup>
        <Folder Include="My Project" />
    </ItemGroup>
</Project>