<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<RootNamespace>VB301</RootNamespace>
		<AssemblyName>VB301</AssemblyName>
		<Version>1.0.0</Version>
		<Authors>Pierre Violent</Authors>
		<Title>VB301 ODBC to SQL Client</Title>
		<Copyright>Copyright Â©2007-2024 Pierre Violent</Copyright>
		<StartupObject>VB301.Program</StartupObject>
		<NoWarn>BC40000</NoWarn>
	</PropertyGroup>
	
	<ItemGroup>
		<Import Include="System.Data" />
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
		<PackageReference Include="System.Data.SqlClient" Version="4.9.0" />
	</ItemGroup>

</Project>
