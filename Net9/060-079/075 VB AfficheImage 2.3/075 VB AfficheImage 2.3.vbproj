<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0-windows</TargetFramework>
		<OutputType>WinExe</OutputType>
		<RootNamespace>AfficheImage2</RootNamespace>
		<AssemblyName>AfficheImage2</AssemblyName>
		<Version>2.3.0</Version>
		<Authors>Pierre Violent</Authors>
		<Title>AfficheImage</Title>
		<Description>Affichage d'images simple</Description>
		<Copyright>Copyright ©1997-2025 Pierre Violent</Copyright>
		<ApplicationIcon>MAIL18.ICO</ApplicationIcon>
		<UseWindowsForms>true</UseWindowsForms>
		<StartupObject>AfficheImage2.frmAfficheImage</StartupObject>
		<ApplicationManifest>My Project\app.manifest</ApplicationManifest>
		<NeutralLanguage>en</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<Import Include="System.Data" />
		<Import Include="System.Drawing" />
		<Import Include="System.Windows.Forms" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="My Project\Application.Designer.vb">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Application.myapp</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<None Update="My Project\Application.myapp">
			<Generator>MyApplicationCodeGenerator</Generator>
			<LastGenOutput>Application.Designer.vb</LastGenOutput>
		</None>
	</ItemGroup>

	<ItemGroup>
		<Folder Include="My Project\" />
	</ItemGroup>

</Project>