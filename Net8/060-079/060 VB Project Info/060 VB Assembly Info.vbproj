<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net8.0-windows</TargetFramework>
        <OutputType>WinExe</OutputType>
        <RootNamespace>VB060</RootNamespace>
        <AssemblyName>VB060</AssemblyName>
        <Version>3.5.0</Version>
        <Authors>Pierre Violent</Authors>
        <Title>Titre de l'assembly</Title>
        <Description>Description de l'assembly</Description>
        <Company>Bonne compagnie</Company>
        <Copyright>Copyright ©2001-2023 Pierre Violent</Copyright>
        <UseWindowsForms>true</UseWindowsForms>
        <StartupObject>VB060.InfosForm</StartupObject>
    </PropertyGroup>

    <ItemGroup>
        <Import Include="System.Data" />
        <Import Include="System.Drawing" />
        <Import Include="System.Windows.Forms" />
    </ItemGroup>

    <ItemGroup>
        <Compile Update="My Project\Application.Designer.vb">
            <DesignTime>True</DesignTime>
            <AutoGen>True</AutoGen>
            <DependentUpon>Application.myapp</DependentUpon>
        </Compile>
    </ItemGroup>

    <ItemGroup>
        <None Update="My Project\Application.myapp">
            <Generator>MyApplicationCodeGenerator</Generator>
            <LastGenOutput>Application.Designer.vb</LastGenOutput>
        </None>
    </ItemGroup>

    <ItemGroup>
        <Folder Include="My Project\" />
    </ItemGroup>

</Project>