<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0-windows</TargetFramework>
		<OutputType>WinExe</OutputType>
		<RootNamespace>PicResize</RootNamespace>
		<AssemblyName>PicResize</AssemblyName>
		<Version>1.0.0</Version>
		<Authors>Pierre Violent</Authors>
		<Title>Simplified Batch Picture Resize Utility</Title>
		<Copyright>Copyright ©2003-2023 Pierre Violent</Copyright>
		<ApplicationIcon>Resize.ico</ApplicationIcon>
		<UseWindowsForms>true</UseWindowsForms>
		<StartupObject>PicResize.frmPicResize</StartupObject>
	</PropertyGroup>

	<ItemGroup>
		<Import Include="System.Data" />
		<Import Include="System.Drawing" />
		<Import Include="System.Windows.Forms" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="My Project\Application.Designer.vb">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>Application.myapp</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<None Update="My Project\Application.myapp">
			<Generator>MyApplicationCodeGenerator</Generator>
			<LastGenOutput>Application.Designer.vb</LastGenOutput>
		</None>
	</ItemGroup>

	<ItemGroup>
		<Folder Include="My Project\" />
	</ItemGroup>

</Project>